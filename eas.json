{
  "cli": {
    "version": ">=3.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "dev",
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "API_BASE_URL": "https:10.0.2.2:8082",
        "KAKAO_REST_API_KEY": "카카오REST_KEY",
        "KAKAO_REDIRECT_URI": "https://8836fd3e7e85.ngrok-free.app/auth/kakao/callback",
        "GOOGLE_CLIENT_ID": "구글_CLIENT_ID",
        "NAVER_CLIENT_ID": "네이버_CLIENT_ID"
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
